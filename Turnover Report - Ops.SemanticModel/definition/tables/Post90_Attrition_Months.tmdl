	partition Post90_Attrition_Months = calculated
		mode: import
		source = ```
				
				VAR BaseAccounts =
				    FILTER(
				        Implementation_table,
				        NOT ISBLANK([Transition Date])
				    )
				
				VAR With90Days =
				    ADDCOLUMNS(
				        BaseAccounts,
				        "TargetDate", [Transition Date] + 90
				    )
				
				VAR AddMonthInfo =
				    ADDCOLUMNS(
				        With90Days,
				        "Post90MonthStart", 
				            DATE(YEAR([TargetDate]), MONTH([TargetDate]), 1),
				       "YearMonth", 
				            FORMAT([TargetDate], "yyyy-MM")
				    )
				
				RETURN
				    SELECTCOLUMNS(
				        AddMonthInfo,
				        "Segmentation_ID", [Segmentation_ID],
				        "Account Name", [Main Account Name],
				        "Business Unit", [Business Unit],
				        "Country", [Country],
				        "YearMonth", [YearMonth],
				        "Date", [Post90MonthStart]
				    )
				
				```

