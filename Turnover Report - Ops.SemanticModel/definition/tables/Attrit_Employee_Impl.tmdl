	partition Attrit_Employee_Impl = calculated
		mode: import
		source = ```
				
				FILTER (
				    ADDCOLUMNS (
				        removals,
				        "Deal Closing", LOOKUPVALUE(
				            Implementation_table[Deal Closing],
				            Implementation_table[Segmentation_ID],
				            removals[Segmentation_ID]
				        ),
				        "Transition Date", LOOKUPVALUE(
				            Implementation_table[Transition Date],
				            Implementation_table[Segmentation_ID],
				            removals[Segmentation_ID]
				        ),
				        "Main Account Name Impl", LOOKUPVALUE(
				            Implementation_table[Main Account Name],
				            Implementation_table[Segmentation_ID],
				            removals[Segmentation_ID]
				        )
				    ),
				    NOT ISBLANK([Deal Closing])
				    &&
				    NOT ISBLANK([Transition Date])
				    &&
				    (YEAR(removals[Finance Report]) * 100 + MONTH(removals[Finance Report]))
				        >= (YEAR([Deal Closing]) * 100 + MONTH([Deal Closing]))
				    &&
				    (YEAR(removals[Finance Report]) * 100 + MONTH(removals[Finance Report]))
				        <= (YEAR([Transition Date]) * 100 + MONTH([Transition Date]))
				)
				
				```

	annotation PBI_Id = 24fb9f730fd440efbfcf80d5a964e4be

