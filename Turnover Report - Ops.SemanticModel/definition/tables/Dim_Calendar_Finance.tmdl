	partition Dim_Calendar_Finance = calculated
		mode: import
		source = ```
				ADDCOLUMNS (
				CALENDAR ( MIN (removals[Finance report]), MAX(removals[Finance report])),
				"#Year", YEAR ( [Date] ),
				"#Month", MONTH ( [Date] ),
				"#Day", DAY ( [Date] ),
				"Month", FORMAT ( [Date], "MMMM" ),
				"ShortMonth", FORMAT ( [Date], "MMM" ),
				"#DayOfTheWeek", WEEKDAY ( [Date],2 ),
				"#WeekOfThYear", WEEKNUM ( [Date],2 ),
