	partition 'Attrition 2' = calculated
		mode: import
		source =
				
				SUMMARIZE(
				    FILTER(
				        removals,
				        NOT ISBLANK(
				            CALCULATE(
				                MAX(customer_segmentation[deal closing date]),
				                FILTER(
				                    customer_segmentation,
				                    customer_segmentation[segmentation_ID] = removals[Segmentation_ID]
				                )
				            )
				        )
				    ),
				    removals[Segmentation_ID],
				    removals[Business Unit],
				    removals[Branch],
				    removals[Country],
				    removals[Service Line],
				    removals[Industry],
				    removals[roster_vs_invoice_ID],
				
				    // Columnas adicionales con agregaci√≥n
				    "Deal Closing", CALCULATE(
				        MAX(customer_segmentation[deal closing date]),
				        FILTER(
				            customer_segmentation,
				            customer_segmentation[segmentation_ID] = removals[Segmentation_ID]
				        )
				    ),
				    "Transition Date", CALCULATE(
				        MAX(customer_segmentation[Transition_ops]),
				        FILTER(
				            customer_segmentation,
				            customer_segmentation[segmentation_ID] = removals[Segmentation_ID]
				        )
				    ),
				    "Main Account Name", CALCULATE(
				        MAX(customer_segmentation[Account Name]),
				        FILTER(
				            customer_segmentation,
				            customer_segmentation[segmentation_ID] = removals[Segmentation_ID]
				        )
				    )
				)

	annotation PBI_Id = c00934993c7a4505874f8416ed2c12cb

